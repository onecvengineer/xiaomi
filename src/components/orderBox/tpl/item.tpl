<div class="order-item clearfix" data-id="{{cartId}}">
  <div class="col id">{{orderId}}</div>
  <a href="{{link}}" title="查看商品详情" class="col img">
    <img src="{{img}}" alt="{{name}}">
  </a>
  <div class="col name">{{name}}</div>
  <div class="col version">{{version}}</div>
  <div class="col color">{{color}}</div>
  <div class="col price">
    <span class="J_price">{{price}}</span>元
  </div>
  <div class="col">{{time}}</div>
</div>